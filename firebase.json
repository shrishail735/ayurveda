{
  "hosting": {
    "public": "dist/ayurveda",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/sitemap.xml",
        "destination": "/sitemap.xml"
    },
    {
        "source": "**",
        "destination": "/index.html"
    }
    ]
  }
}
